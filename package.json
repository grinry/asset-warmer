{
  "name": "asset-warmer",
  "version": "0.1.0",
  "description": "Script for preloading website with percentage progress.",
  "main": "./src/asset-warmer",
  "scripts": {
    "webpack": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "dev": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "hmr": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grinry/asset-warmer.git"
  },
  "keywords": [
    "javascript",
    "assets",
    "warmer"
  ],
  "author": "Rytis Grincevicius <rytis.grincevicius@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiberzauras/asset-warmer/issues"
  },
  "typings": "./asset-warmer.d.ts",
  "homepage": "https://github.com/kiberzauras/asset-warmer#readme",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6"
  }
}
